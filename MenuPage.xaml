<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="Project2.MenuPage"
                  xmlns:local="clr-namespace:Project2;assembly=Project2"
                 >
  
  <MasterDetailPage.Master>
    <ContentPage Title="Menu">
      <StackLayout Orientation="Vertical">
      <Button Text="Login/Sign up" />
        <Button Text="Survey" Clicked="Survey_OnClicked"/>
        <Button Text="Search your favourite" Clicked="Search_OnClicked"/>
        <Button Text="Be A Barista" Clicked="Barista_OnClicked"/>
        <Button Text="Animation" Clicked="Animation_OnClicked"/>
        <Button Text="About us" Clicked="About_OnClicked"/>
      </StackLayout>
     </ContentPage>
    </MasterDetailPage.Master>
  
  <MasterDetailPage.Detail>
    <local:Homepage/>
  </MasterDetailPage.Detail>

</MasterDetailPage>